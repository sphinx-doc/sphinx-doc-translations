msgid "``'exclude-members'``: See :rst:dir:`automodule:exclude-members`."
msgstr "``'exclude-members'``：见 :rst:dir:`automodule:exclude-members`。"

#: ../../sphinx/doc/usage/extensions/autodoc.rst:1122
msgid "``'ignore-module-all'``: See :rst:dir:`automodule:ignore-module-all`."
msgstr "``'ignore-module-all'``：见 :rst:dir:`automodule:ignore-module-all`。"

#: ../../sphinx/doc/usage/extensions/autodoc.rst:1123
msgid "``'member-order'``: See :rst:dir:`automodule:member-order`."
msgstr "``'member-order'``：见 :rst:dir:`automodule:member-order`。"

#: ../../sphinx/doc/usage/extensions/autodoc.rst:1124
msgid "``'show-inheritance'``: See :rst:dir:`autoclass:show-inheritance`."
msgstr "``'show-inheritance'``：见 :rst:dir:`autoclass:show-inheritance`。"

#: ../../sphinx/doc/usage/extensions/autodoc.rst:1125
msgid "``'class-doc-from'``: See :rst:dir:`autoclass:class-doc-from`."
msgstr "``'class-doc-from'``：见 :rst:dir:`autoclass:class-doc-from`。"

